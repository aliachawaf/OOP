package fr.igpolytech.aliachawaf.Battleship;

public class HumanPlayer extends Player {

	private String name;

	//constructor
	public HumanPlayer(int mapSize, String name) {
		super(mapSize);
		this.name = name;
	}

	// getters & setters
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
	
	
	
}
